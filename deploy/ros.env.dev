# Update GitHub repos with git pull on container startup.
export UPDATE_REPO=yes

# Port to run Ros server.
export ROS_PORT=5002

# Neo4J database connection parameters.
export NEO4J_HOST=localhost
export NEO4J_USERNAME=neo4j
export NEO4J_PASSWORD=neo4j0
export NEO4J_BOLT_PORT=7687
export NEO4J_WEB_PORT=7474

# Redis connection parameters.
export REDIS_HOST=localhost
export REDIS_PORT=6379